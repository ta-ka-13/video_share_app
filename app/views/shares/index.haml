= render 'shared/header'
.wrapper
  .video-inner
    .video
      - @shares.each do |share|
        %p
          -# = share.title
          -# = image_tag(share.video_url(:screenshot).to_s, id: "video", :alt => "screenshot")
          -# = image_tag(share.video_url.to_s, id: "video")
          = link_to share do
            %span.fa.fa-film
        %p

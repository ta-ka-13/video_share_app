-# = render 'shared/header'
.show-main
  = current_user.nickname
.list
  動画投稿リスト
.video
  - @shares.each do |share|
    = image_tag(share.video_url.to_s, id: "video")
    = share.title
    = link_to '視聴', share
    = link_to '編集', share_path(share)
    = link_to '削除', share, method: :delete, data: { confirm: '本当に削除してよろしいですか？' }


.home
  = link_to 'HOME', shares_path(@share),class: "EDITLink"
